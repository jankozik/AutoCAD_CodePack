<Window x:Class="Dreambuild.AutoCAD.MultiInputs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Height="300" Width="400" Background="WhiteSmoke" WindowStartupLocation="CenterScreen" ShowInTaskbar="False" Title="Inputs" ResizeMode="NoResize">
    <Grid>
        <Border BorderBrush="Gray" BorderThickness="1" Margin="12,12,12,41">
            <ScrollViewer Background="White">
                <Border BorderBrush="Gray" BorderThickness="0,0,1,0">
                    <StackPanel x:Name="PropertyList" Margin="5,0,0,0">

                    </StackPanel>
                </Border>
            </ScrollViewer>
        </Border>
        <Button Content="OK" Height="23" HorizontalAlignment="Right" Margin="0,0,93,12" VerticalAlignment="Bottom" Width="75" Click="OkButton_Click" />
        <Button Content="Cancel" Margin="0,0,12,12" Height="23" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" Click="CancelButton_Click" />
    </Grid>
</Window>
